
<%= form_with model: @listing do |f| %>
    <%=f.label :location %>
    <%=f.collection_select :city_id, @cities. :id. :name, inclue_hidden: false %>

    <%=f.label :length_of_trip %>
    <%=f.number_field :length_of_stay %>

    <%= f.label :daily_rate %>
    <%= f.select :compensation, [15, 20, 25, 30, 40, 50, 100] %>

    <%=f.collection_select :parent_id, @parents, :id, :name, inclue_hidden: false %>

    <%=f.submit "Create Listing" %>
<% end %>
